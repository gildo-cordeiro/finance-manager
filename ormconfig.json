{
  "type": "postgres",
  "host": "${DB_HOST}",
  "port": "${DB_PORT}",
  "username": "${DB_USERNAME}",
  "password": "${DB_PASSWORD}",
  "database": "${DB_DATABASE}",
  "entities": [
    "dist/**/*.entity.js"
  ],
  "migrations": [
    "dist/**/migrations/*.js"
  ],
  "cli": {
    "migrationsDir": "./src/shared/database/migrations/"
  }
}